<% @products.each do |product| %>
<%=link_to product.name, show_product_path(@product) %>, ><br/>
<%=product.kind %>  <br/>
	<%=product.description %> <br/>
	<%=product.artist %> <br/>
	<%=product.price %> <br/>
	<%=product.quantity %> <br/>
	<%=product.tagline %> <br/>
<img src="<%=product.image.first %>">	<br/>
	<br/>
<% end %>